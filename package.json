{
  "name": "webpack-wordpress",
  "author": "Sloan Sparger",
  "version": "0.0.0",
  "description": "Build Wordpress themes using Webpack and Hot Module Reloading",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.14.0",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "2.1.6",
    "css-loader": "^0.28.5",
    "eslint": "^4.5.0",
    "eslint-loader": "^1.9.0",
    "file-loader": "^0.11.2",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "scripts": {
    "build": "webpack --config webpack-configs/webpack.prod.js",
    "start": "webpack-dev-server --port=3000 --config webpack-configs/webpack.dev.js",
    "setup": "wp core download --path='./wordpress'"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parser": "babel-eslint",
    "rules": {
      "strict": 0
    }
  }
}
